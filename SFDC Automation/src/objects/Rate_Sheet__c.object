    <validationRules>
        <fullName>No_UpdatesOnRateSheet</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISNEW()), ISCHANGED( Name ) || ISCHANGED( Rate_Sheet_Name__c ) ||  ISCHANGED( Product_Line__c ) || ISCHANGED( Package_Type__c ) || ISCHANGED( Gross_Revenue__c ) || ISCHANGED( No_of_Vehicles__c )  ||  ISCHANGED( EVA__c ) || ISCHANGED( ext_ratesheet_id__c ) || ISCHANGED( Deal_Summary__c ) || ISCHANGED( Transaction_Type__c ) || ISCHANGED( Branch_No__c ) || ISCHANGED( Old_Revenue__c ) || ISCHANGED( FuelType__c )) &amp;&amp; AND(NOT($Profile.Name = &apos;System Administrator&apos;), NOT($Profile.Name = &apos;SSO System Administrator&apos;), NOT($Profile.Name = &apos;Business Admin&apos;), NOT($Profile.Name = &apos;Business Admin Non SSO&apos;), NOT($Profile.Name = &apos;Business Support&apos;))</errorConditionFormula>
        <errorMessage>No changes are allowed on this page. if you are trying to update the Include in Quote, please go to the Opportunity Details Page. For any other updates, go to Radar using the Edit in Radar link on the Opportunity Details Page.</errorMessage>
    </validationRules>
