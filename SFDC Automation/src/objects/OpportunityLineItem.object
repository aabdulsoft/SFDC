    <validationRules>
        <fullName>CustomerTransactionTypeNew</fullName>
        <active>true</active>
        <description>Validates that Transaction Type for Prospects cannot be New.</description>
        <errorConditionFormula>(ISPICKVAL( Transaction_Type__c , &quot;New Customer&quot;)) &amp;&amp;  ISPICKVAL(Opportunity.Account.Type, &quot;Customer&quot;)</errorConditionFormula>
        <errorDisplayField>Transaction_Type__c</errorDisplayField>
        <errorMessage>You cannot use New Customer transaction type with Customer Accounts.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OpportunityEstimatedCloseDateInPast</fullName>
        <active>true</active>
        <description>Reviews the Opportunity Stage and Estimate Close Date. If it is not closed, the Estimate Close Date cannot be in the past, except for ETL User</description>
        <errorConditionFormula>AND((AND(NOT(ISPICKVAL( Opportunity.StageName , &quot;Lost&quot;)), 
NOT(ISPICKVAL(Opportunity.StageName, &quot;Booked&quot;))) &amp;&amp; Opportunity.CloseDate&lt;TODAY()), $User.Id != &apos;00560000001NIEp&apos;,  $Profile.Name !=&apos;System Administrator&apos;, $Profile.Name != &apos;SSO System Administrator&apos;, OppRecordTypeName__c != &apos;UVS Opportunity&apos;)</errorConditionFormula>
        <errorDisplayField>OpportunityId</errorDisplayField>
        <errorMessage>Estimated Close Date for Opportunity is in past, Please select an Estimated Close Date in the future.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ProductLineMustEqualProduct</fullName>
        <active>true</active>
        <description>Validate that picklist value matches selected Product</description>
        <errorConditionFormula>AND(NOT(Opportunity.RecordType.Name = &apos;National Rental Opportunity&apos;) , OR(
AND(ISPICKVAL(Product2.Family, &quot;FSL&quot;), NOT(ISPICKVAL( Product_Line__c ,&quot;FSL&quot;))),
AND(ISPICKVAL(Product2.Family, &quot;Fuel&quot;), NOT(ISPICKVAL( Product_Line__c ,&quot;Fuel&quot;))),
AND(ISPICKVAL(Product2.Family, &quot;RPM&quot;), NOT(ISPICKVAL( Product_Line__c ,&quot;RPM&quot;))),
AND(ISPICKVAL(Product2.Family, &quot;Rydesmart&quot;), NOT(ISPICKVAL( Product_Line__c ,&quot;Rydesmart&quot;))),
AND(ISPICKVAL(Product2.Family, &quot;Insurance&quot;), NOT(ISPICKVAL( Product_Line__c ,&quot;Insurance&quot;))),
AND(ISPICKVAL(Product2.Family, &quot;DCC Light&quot;), NOT(ISPICKVAL( Product_Line__c ,&quot;DCC Light&quot;)))
))</errorConditionFormula>
        <errorDisplayField>Product_Line__c</errorDisplayField>
        <errorMessage>The Product Line must match the selected product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ProspectTransactionTypeNew</fullName>
        <active>true</active>
        <description>Validates that Transaction Type for Prospects can only be New.</description>
        <errorConditionFormula>AND ((Opportunity.RecordType.DeveloperName  != &apos;UVS_Opportunity&apos;),(AND(ISPICKVAL(Opportunity.Account.Type, &quot;Prospect&quot;),  ISNULL(Opportunity.Account.ParentId), NOT(ISPICKVAL( Transaction_Type__c , &quot;New Customer&quot;)))
||
AND(
ISPICKVAL( Opportunity.Account.Parent.Type, &quot;Prospect&quot;), NOT(ISPICKVAL( Transaction_Type__c , &quot;New Customer&quot;)))))</errorConditionFormula>
        <errorDisplayField>Transaction_Type__c</errorDisplayField>
        <errorMessage>Prospect Accounts may only use the New Customer transaction type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RenewalOppTransactionTypes</fullName>
        <active>false</active>
        <description>Determines if there is a Renewal Opp, and does not allow transaction type values other than Replacement or Term Extension</description>
        <errorConditionFormula>NOT(ISBLANK( Unit_Number__c  ))  &amp;&amp;  not(
OR(ISPICKVAL(Transaction_Type__c, &quot;Replacement&quot;), ISPICKVAL(Transaction_Type__c, &quot;Term Extension&quot;))
)</errorConditionFormula>
        <errorDisplayField>Transaction_Type__c</errorDisplayField>
        <errorMessage>Renewal Opportunities can only have a transaction type of Replacement or Term Extension.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TransactionTypesWithUsedVehicleType</fullName>
        <active>true</active>
        <description>Requires that a Product have the &apos;Used&apos; value in the Vehicle Type when the Transaction Type on Oppty Line Item is Term Extension or Rate Change</description>
        <errorConditionFormula>NOT(ISPICKVAL(Product2.VHC_TYPE__c, &quot;Used&quot;)) &amp;&amp;
OR(ISPICKVAL(Transaction_Type__c, &quot;Rate Change&quot;), 
ISPICKVAL(Transaction_Type__c, &quot;Term Extensiont&quot;))</errorConditionFormula>
        <errorDisplayField>Transaction_Type__c</errorDisplayField>
        <errorMessage>&apos;Rate Change&apos; or &apos;Term Extension&apos; can only have products with a Used vehicle type.</errorMessage>
    </validationRules>
