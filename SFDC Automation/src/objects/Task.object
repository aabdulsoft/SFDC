    <validationRules>
        <fullName>AssignedTo_OnlyAdminManagers</fullName>
        <active>true</active>
        <description>The Assigned To field on the Survey Task can only be updated by System Administrator or Service Management Profile</description>
        <errorConditionFormula>AND(
   ISPICKVAL(Type,&quot;Survey&quot;),Subject = &quot;Customer Sat Survey&quot;, 
   ISCHANGED( OwnerId),
     $UserRole.Name &lt;&gt; &quot;Service Team Management&quot;,
     NOT($Profile.Name = &quot;System Administrator&quot;) 
)</errorConditionFormula>
        <errorDisplayField>OwnerId</errorDisplayField>
        <errorMessage>Must be a manager to revise this field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CallbackRequiresSurveyCallDateTime</fullName>
        <active>true</active>
        <description>The Callback Survey Result requires a survey call date/time value.</description>
        <errorConditionFormula>AND
(ISBLANK(Survey_Call_Date_Time__c ),
OR(ISPICKVAL(Survey_Call_Result__c, &quot;Future Reschedule&quot;),  ISPICKVAL(Survey_Call_Result__c, &quot;Same Day Reschedule&quot;)))</errorConditionFormula>
        <errorDisplayField>Survey_Call_Date_Time__c</errorDisplayField>
        <errorMessage>The requested callback date/time must be entered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EnforceCategory_DeferredCallbackorRetry</fullName>
        <active>true</active>
        <description>If agent chooses a status of Deferred/Callback, enforce the addition of either Same Day Reschedule or Future Reschedule. If agent chooses a status of Retry, enforce the addition of one of the dependent value choices.</description>
        <errorConditionFormula>AND
(ISPICKVAL(Type,&quot;Survey&quot;),Subject = &quot;Customer Sat Survey&quot;, ISPICKVAL(Survey_Call_Result__c, &quot;&quot;),
OR (ISPICKVAL( Status, &quot;Deferred/Callback&quot;),
ISPICKVAL(Status, &quot;Retry&quot;)))</errorConditionFormula>
        <errorDisplayField>Survey_Call_Result__c</errorDisplayField>
        <errorMessage>Please select a value in the Survey Call Result field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EnforceNoRefusals_SafetyKleenSurveys</fullName>
        <active>true</active>
        <description>Agent is not allowed to choose Refusal for any Safety-Kleen survey.</description>
        <errorConditionFormula>AND
(ISPICKVAL( Survey_Sub_Type__c, &apos;SAFETY-KLEEN&apos;),ISPICKVAL(Type,&quot;Survey&quot;),Subject = &quot;Customer Sat Survey&quot;,
OR(ISPICKVAL(Survey_Call_Result__c, &apos;Refusal&apos;),ISPICKVAL(Survey_Call_Result__c, &apos;Refusal - Too Busy&apos;)))</errorConditionFormula>
        <errorDisplayField>Survey_Call_Result__c</errorDisplayField>
        <errorMessage>Safety-Kleen surveys are not allowed to have refusals.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_OnlyAdminManagers</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL(Type,&quot;Survey&quot;),Subject = &quot;Customer Sat Survey&quot;, 
ISCHANGED( Status), 
$UserRole.Name &lt;&gt; &quot;Service Team Management&quot;, 
$UserRole.Name &lt;&gt; &apos;Service Team&apos;, 
NOT($Profile.Name = &quot;System Administrator&quot;) 
)</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>Must be an agent or Administrator to change this field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SurveyCallDateTime_NoBackDate</fullName>
        <active>true</active>
        <description>The Survey Call Date Time should always be a date in the future.</description>
        <errorConditionFormula>NOT( ISBLANK(Survey_Call_Date_Time__c))  &amp;&amp;  DATEVALUE(Survey_Call_Date_Time__c) &lt; Today()   &amp;&amp;
NOT($Profile.Name = &quot;System Administrator&quot;)</errorConditionFormula>
        <errorDisplayField>Survey_Call_Date_Time__c</errorDisplayField>
        <errorMessage>The Survey Call Date Time must be a date/time in the future.</errorMessage>
    </validationRules>
