    <validationRules>
        <fullName>Only_Admin_Can_Change_Previous_Yr_Quota</fullName>
        <active>true</active>
        <description>This rule prevents sales people from changing the quotas for previous years when they are working on the current year quotas.</description>
        <errorConditionFormula>AND(NOT($Profile.Name = &apos;System Administrator&apos;), NOT($Profile.Name = &apos;SSO System Administrator&apos;), 
NOT($Profile.Name = &apos;Business Support Non SSO&apos;), 
NOT($Profile.Name = &apos;Business Admin&apos;), 
NOT($Profile.Name = &apos;Business Admin Non SSO&apos;)) 
 &amp;&amp; YEAR( From_Date__c) &lt; YEAR ( TODAY() )</errorConditionFormula>
        <errorMessage>Only Salesforce Administrators can edit a quota for a previous year.</errorMessage>
    </validationRules>
