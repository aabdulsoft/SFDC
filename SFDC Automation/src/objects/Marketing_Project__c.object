    <fields>
        <fullName>Project_Days__c</fullName>
        <description>Numbers of days the project lasted or it has been opened if still open. The assumption of completion is a value in the Actual Completion Date field</description>
        <externalId>false</externalId>
        <formula>if (isnull(Actual_Completion_Date__c),
(TODAY() - IF(isnull(Start_Date__c), today(), Start_Date__c)),
(Actual_Completion_Date__c - Start_Date__c + 1))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Project Days</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
