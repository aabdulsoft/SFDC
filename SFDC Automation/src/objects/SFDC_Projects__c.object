    <allowInChatterGroups>false</allowInChatterGroups>
    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <deploymentStatus>Deployed</deploymentStatus>
    <description>Object to be used by Sales Operations, IT and other areas that may be working on projects related to SFDC. The intention is to have a repository of tasks where the team posts what they are doing around these projects. It may end up being a basic project control mechanism but the initial intention is to log activities and comments.</description>
    <enableActivities>true</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableFeeds>true</enableFeeds>
    <enableHistory>true</enableHistory>
    <enableReports>true</enableReports>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <fields>
        <fullName>Assigned_To__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>Main responsible individual for this project</description>
        <externalId>false</externalId>
        <label>Assigned To</label>
        <referenceTo>User</referenceTo>
        <relationshipName>SFDC_Projects</relationshipName>
        <required>false</required>
        <trackFeedHistory>true</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Cost_ROI__c</fullName>
        <description>Brief description of the estimated cost and ROI for the project. Details need to be included in either an attachment or a chat to the project</description>
        <externalId>false</externalId>
        <label>Cost / ROI</label>
        <length>1500</length>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>LongTextArea</type>
        <visibleLines>3</visibleLines>
    </fields>
    <fields>
        <fullName>Estimated_Finish_Date__c</fullName>
        <externalId>false</externalId>
        <label>Estimated Finish Date</label>
        <required>false</required>
        <trackFeedHistory>true</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Main_Benefits__c</fullName>
        <description>Brief description of the benefits. Additional details need to be added either as an attachment or in a chat to the object</description>
        <externalId>false</externalId>
        <label>Main Benefits</label>
        <length>1500</length>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>LongTextArea</type>
        <visibleLines>3</visibleLines>
    </fields>
    <fields>
        <fullName>Priority__c</fullName>
        <description>Indicates the priority of this project for the person that it has been assigned to. If multiple projects have the same priority, it indicates that the person responsible is expected to work on them at the same time. Use the decimal to indicate priorities within those</description>
        <externalId>false</externalId>
        <label>Priority</label>
        <precision>5</precision>
        <required>false</required>
        <scale>2</scale>
        <trackFeedHistory>true</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Scope__c</fullName>
        <description>Brief description of the main areas the project will and will not cover</description>
        <externalId>false</externalId>
        <label>Scope</label>
        <length>1500</length>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>LongTextArea</type>
        <visibleLines>3</visibleLines>
    </fields>
    <fields>
        <fullName>Status__c</fullName>
        <externalId>false</externalId>
        <label>Status</label>
        <picklist>
            <picklistValues>
                <fullName>Proposed</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Accepted</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>In Progress</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Completed</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Discarded</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackFeedHistory>true</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>WeeklyMeeting__c</fullName>
        <defaultValue>false</defaultValue>
        <description>To Identify topics related to SFDC Weekly Meeting</description>
        <externalId>false</externalId>
        <label>WeeklyMeeting</label>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>SFDC Project</label>
    <listViews>
        <fullName>All</fullName>
        <columns>NAME</columns>
        <columns>OWNER.LAST_NAME</columns>
        <columns>Estimated_Finish_Date__c</columns>
        <columns>Status__c</columns>
        <columns>LAST_ACTIVITY</columns>
        <columns>Assigned_To__c</columns>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
    <listViews>
        <fullName>All_Open</fullName>
        <columns>NAME</columns>
        <columns>OWNER.LAST_NAME</columns>
        <columns>Assigned_To__c</columns>
        <columns>Estimated_Finish_Date__c</columns>
        <columns>Status__c</columns>
        <columns>LAST_ACTIVITY</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Status__c</field>
            <operation>notEqual</operation>
            <value>Completed,Discarded</value>
        </filters>
        <label>All Open</label>
    </listViews>
    <listViews>
        <fullName>Weekly_Meeting</fullName>
        <columns>NAME</columns>
        <columns>Priority__c</columns>
        <columns>Status__c</columns>
        <columns>OWNER.FIRST_NAME</columns>
        <columns>LAST_UPDATE</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>WeeklyMeeting__c</field>
            <operation>equals</operation>
            <value>1</value>
        </filters>
        <label>Weekly Meeting</label>
    </listViews>
    <nameField>
        <label>SFDC Projects Name</label>
        <trackFeedHistory>false</trackFeedHistory>
        <trackHistory>false</trackHistory>
        <type>Text</type>
    </nameField>
    <pluralLabel>SFDC Projects</pluralLabel>
    <searchLayouts>
        <customTabListAdditionalFields>Main_Benefits__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>Assigned_To__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>CREATEDBY_USER</customTabListAdditionalFields>
        <customTabListAdditionalFields>CREATED_DATE</customTabListAdditionalFields>
        <searchResultsAdditionalFields>Main_Benefits__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>Assigned_To__c</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>CREATEDBY_USER</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>CREATED_DATE</searchResultsAdditionalFields>
    </searchLayouts>
    <sharingModel>ReadWrite</sharingModel>
</CustomObject>
